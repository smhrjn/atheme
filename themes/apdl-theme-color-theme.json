			"scope": "keyword.operator.new",
			"settings": {
				"fontStyle": "underline",
				"foreground": "abcdef"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "none",
				"foreground": "#ccff66"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"fontStyle": "none",
				"foreground": "#7cff00"
			}
		},
		{
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#6272a4"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffb86c"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#bd93f9"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#f1fa8c",
				"fontStyle": "italic"
			}
