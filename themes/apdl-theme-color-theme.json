				"foreground": "#ffb86c"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#f8f8f2"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#8be9fd"
			}
