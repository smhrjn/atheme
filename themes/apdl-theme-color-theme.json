		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ffb86c"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"fontStyle": "bold",
				"background": "#00000",
				"foreground": "#ff3300"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#bd93f9",
				"foreground": "#f8f8f2"
			}
		},
		{
			"scope": "entity.name.filename",
			"settings": {
				"foreground": "#f1fa8c"
			}
		},
		{
			"scope": "markup.error",
			"settings": {
				"foreground": "#ff5555"
			}
		},
