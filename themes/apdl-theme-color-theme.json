			"scope": "variable.language.this",
			"settings": {
				"foreground": "#8be9fd"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffb86c"
			}
		},
		{
			"scope": [
				"keyword",
				"punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#f4a460"
			}
		},
